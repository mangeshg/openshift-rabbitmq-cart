#!/bin/bash

set -e
set -u

echo "OPENSHIFT_RABBITMQ_BROKER_HOST=${OPENSHIFT_ERL_IP}"
echo "OPENSHIFT_RABBITMQ_PORT=${OPENSHIFT_RABBITMQ_PORT}"
echo "OPENSHIFT_RABBITMQ_USERNAME=${OPENSHIFT_RABBITMQ_USERNAME}"
echo "OPENSHIFT_RABBITMQ_PASSWORD=${OPENSHIFT_RABBITMQ_PASSWORD}"
echo "OPENSHIFT_RABBITMQ_URI=amqp://${OPENSHIFT_RABBITMQ_USERNAME}:${OPENSHIFT_RABBITMQ_PASSWORD}@${OPENSHIFT_ERL_IP}:${OPENSHIFT_RABBITMQ_PORT}//"
echo "OPENSHIFT_RABBITMQ_ADMIN_PORT=${OPENSHIFT_RABBITMQ_ADMIN_PROXY_PORT}"
